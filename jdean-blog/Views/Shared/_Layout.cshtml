</head>
<body>
    <!-- Sidenav -->
    <section class="sidenav">
        <img src="~/Assets/images/sidenav-img03.jpg" alt="" class="img-responsive sidenav-img">
        <p class="sidenav-desc">Thanks for stopping by.</p>
        <h6 class="mt-40">Get in Touch</h6>
        <address class="info-address">Downtown<br>Greensboro, NC  </address>
        <p>Phone: + 1.316.789.3704</p>
        <p>Email: <a href="mailto:jessicadeanblog@gmail.com">jessicadeanblog@gmail.com</a></p>
        <p></p>
        <div class="social-icons nobase dark mt-40">
            <a href="#"><i class="fa fa-twitter"></i></a>
            <a href="#"><i class="fa fa-instagram"></i></a>
            
            <a href="#"><i class="fa fa-linkedin"></i></a>
            
        </div>
        <a href="#" id="sidenav-close">
            <i class="fa fa-times"></i>
        </a>
    </section> <!-- end sidenav -->
    <!-- Main Wrapper -->
    <div class="main-wrapper">
        <!-- Header -->
        <header>
            <div class="main-menu">
                <nav class="navbar" id="main-nav">
                    <div class="container">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="navbar-header">
                                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
                                        <span class="sr-only">Toggle navigation</span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                    </button>
                                    <!-- Change Logo Here -->
                                    <a class="navbar-brand" data-scroll="" href="#header">Jessica<span>Dean</span><i class="fa fa-code"></i></a>
                                </div>
                                <!-- Fullscreen search -->
                                <div class="search-wrap">
                                    <div class="search-inner">
                                        <div class="search-cell">
                                            <form method="get">
                                                <div class="search-field-holder">
                                                    <input type="search" class="form-control main-search-input" placeholder="Type & Hit Enter...">
                                                    <i class="fa fa-times" id="search-close"></i>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div> <!-- end fullscreen search -->
                                <div class="collapse navbar-collapse" id="defaultNavbar1">
                                    <ul class="nav navbar-nav pull-right">
                                        <li class="active"><a @*data-scroll*@ href="http://www.jdean-portfolio.azurewebsites.net">Home</a></li>
                                        <li><a href="http://jdean-portfolio.azurewebsites.net/#about" target="_blank">About</a></li>
                                        <li><a href="http://jdean-portfolio.azurewebsites.net/assets/documents/jessicadean1.pdf" target="_blank">Resume</a></li>
                                        <li><a href="http://jdean-portfolio.azurewebsites.net/#works" target="_blank">Portfolio</a></li>
                                        <li>@Html.ActionLink("Blog","Index","Posts")</li>
                                        <li>@Html.ActionLink("Contact","Contact","Home")</li>
                                        <li class="nav-right hidden-xs">
                                            <ul class="list-unstyled">
                                                <li class="nav-search-wrap hidden-xs">
                                                    <a href="#" class="nav-search search-trigger">
                                                        <i class="fa fa-search"></i>
                                                    </a>
                                                </li>
                                                <li class="nav-icon-wrap hidden-sm hidden-xs">
                                                    <div id="nav-icon">
                                                        <div class="nav-icon-inner">
                                                            <a href="#" id="nav-icon-trigger" class="nav-icon-trigger">
                                                                <span></span>
                                                                <span></span>
                                                                <span></span>
                                                                <span></span>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </li>
                                            </ul>
                                        </li><!-- Ends .nav-right -->
                                        <li id="mobile-search" class="hidden-lg hidden-md hidden-sm">
                                            <form method="get" class="mobile-search">
                                                <input type="search" class="form-control" placeholder="Search...">
                                                <button type="submit" class="search-button">
                                                    <i class="fa fa-search"></i>
                                                </button>
                                            </form>
                                        </li>
                                    </ul>
                                </div><!-- .navbar-collapse -->
                            </div><!-- .container -->
                        </div>
                    </div>
                </nav>
            </div><!-- Ends: .main-menu -->
        </header><!-- Ends: header -->
        <!-- Page Header -->
        <section class="page-heading">
            <div class="page-heading-inner">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-4 page-name">
                            <h2><span>Blog</span> Page</h2>
                            <ul class="list-unstyled breadcrumb">
                                <li><a href="">Home</a></li>
                                <li class="active">Blog</li>
                            </ul>                   </div>
                       
                        <div class="col-sm-8 page-desc">
                            <h2>Find My All <span>Blog Posts</span> Here</h2>
                            <div>
                                <section id="login">
                                    @Html.Partial("_LoginPartial")
                                </section>
                            </div>
                            
                        </div>
                    </div>
                </div>
            </div>
        </section><!-- Ends: .page-heading -->
        <!-- Blog Container -->
        <div class="blog-single-wrapper">
            <div class="container">
                <div class="row">
                    
                    <!-- Blog Content -->
                            
                            <!-- Blog Content -->
                            <div class="col-sm-10 col-sm-offset-1">
                                <div class="blog-single-inner">
                                    @*<div class="blog-single-post">
                                        <div class="post-image">
                                            <img src="~/Assets/images/blog-post01.jpg" alt="" class="img-responsive">
                                        </div>
                                        <div class="post-excerpt">
                                            <div class="row">
                                                <div class="col-sm-5">
                                                    <h2 class="blog-title"><a href="">Blog Post Title Goes Here</a></h2>
                                                    <ul class="list-unstyled post-meta">
                                                        <li>John Smith</li>
                                                        <li><a href="">Category</a></li>
                                                        <li>23 Jul 2017</li>
                                                    </ul>
                                                    <a href="" class="post-details-btn">Read More</a>
                                                </div>
                                                <div class="col-sm-7">
                                                    <p> </p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>*@<!-- Ends: .blog-single-post -->
@RenderBody()
                                    @*!-- Ends: .blog-single-post -->*@
                                    <div class="blog-pagination" hidden>
                                        <ul class="list-unstyled">
                                            <li><a href=""><i class="fa fa-angle-left"></i></a></li>
                                            <li class="active"><a href="">1</a></li>
                                            <li><a href="">2</a></li>
                                            <li><a href="">3</a></li>
                                            <li><a href=""><i class="fa fa-angle-right"></i></a></li>
                                        </ul>
                                    </div>
                                </div><!-- Ends: .blog-inner -->
                            </div>
                        </div>
            </div>
        </div>

        <!-- Footer -->
        <footer>
            <div class="container">
                <div class="row">
                    <div class="col-sm-12 footer-content wow fadeIn">
                        <p>&copy; 2017 All rights reserved | <i class="fa fa-heart"></i>Jessica Dean</p>
                    </div>
                </div>
            </div>
        </footer><!-- Ends: footer -->
    </div><!-- Ends .main-wrapper -->
