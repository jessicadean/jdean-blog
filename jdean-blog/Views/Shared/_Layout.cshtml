<!doctype html>
<html class="no-js" lang="zxx">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Jessica Dean | Blog</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @*@Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")*@
    <!-- Favicon -->
    <link rel="shortcut icon" href="~/Assets/images/favicon4.ico" type="image/x-icon">
    <link rel="icon" href="~/Assets/images/favicon4.ico" type="image/x-icon">

    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/Assets/css/assets/bootstrap.min.css">
